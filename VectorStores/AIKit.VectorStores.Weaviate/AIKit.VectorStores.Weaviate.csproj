<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>AIKit.VectorStores.Weaviate</PackageId>
		<Version>1.0.0</Version>
		<Authors>AIKit Team</Authors>
		<Description>Weaviate vector store provider for AIKit.</Description>
		<PackageTags>AI, VectorStore, Weaviate</PackageTags>
		<RepositoryUrl>https://github.com/durmisi/AIKit</RepositoryUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Options" Version="10.0.1" />
		<PackageReference Include="Microsoft.Extensions.AI" Version="10.1.1" />
		<PackageReference Include="Microsoft.SemanticKernel.Connectors.Weaviate" Version="1.68.0-preview" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\AIKit.Core.VectorStores\AIKit.Core.VectorStores.csproj" />
	</ItemGroup>

</Project>