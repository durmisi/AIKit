<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>AIKit.VectorStores.Elasticsearch</PackageId>
		<Authors>AIKit Team</Authors>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<Description>Elasticsearch vector store provider for AIKit.</Description>
		<PackageTags>AI, VectorStore, Elasticsearch</PackageTags>
		<RepositoryUrl>https://github.com/durmisi/AIKit</RepositoryUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<NoWarn>SKEXP0020</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Elastic.SemanticKernel.Connectors.Elasticsearch" Version="0.3.1" />
		<PackageReference Include="Elastic.Clients.Elasticsearch" Version="9.2.3" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="10.0.2" />
		<PackageReference Include="Microsoft.Extensions.AI" Version="10.2.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\AIKit.VectorStores\AIKit.VectorStores.csproj" />
	</ItemGroup>

</Project>