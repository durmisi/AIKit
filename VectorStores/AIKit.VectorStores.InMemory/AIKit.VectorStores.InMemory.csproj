<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>AIKit.VectorStores.InMemory</PackageId>
		<Authors>Admir Durmishi</Authors>
		<Description>In-memory vector store provider for AIKit.</Description>
		<PackageTags>AI, VectorStore, InMemory</PackageTags>
		<RepositoryUrl>https://github.com/durmisi/AIKit</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>NU5104</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Options" Version="10.0.2" />
		<PackageReference Include="Microsoft.SemanticKernel.Connectors.InMemory" Version="1.68.0-preview" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\AIKit.VectorStores\AIKit.VectorStores.csproj" />
	</ItemGroup>

</Project>