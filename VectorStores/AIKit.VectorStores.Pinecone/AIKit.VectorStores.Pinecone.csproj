<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>AIKit.VectorStores.Pinecone</PackageId>
		<Authors>AIKit Team</Authors>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<Description>Pinecone vector store provider for AIKit.</Description>
		<PackageTags>AI, VectorStore, Pinecone</PackageTags>
		<RepositoryUrl>https://github.com/durmisi/AIKit</RepositoryUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.SemanticKernel.Connectors.Pinecone" Version="1.68.0-preview" />
		<PackageReference Include="Pinecone" Version="2.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\AIKit.Core.VectorStores\AIKit.Core.VectorStores.csproj" />
	</ItemGroup>

</Project>